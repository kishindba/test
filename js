var tables = document.querySelectorAll('.customer');
var startingBalance = document.getElementsByClassName('starting_balance');
var transaction = document.getElementsByClassName('transaction');
var finalBalamce = document.getElementsByClassName('final_balance');

for (var item in tables) {

	for (; i < item.length; i++)
	{
		finalBalance.innerHTML = parseFloat(startingBalance) + parseFloat(transaction);

	}


}
